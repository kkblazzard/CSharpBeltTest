@model User

<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<link rel="stylesheet" type="text/css" media="screen" href="~/css/User/index.css"/>

<title>Login or Register</title>

    <div class=registration>
        <h2>Welcome to the Dojo Activity Center</h2><br>
        <br>
        <h4>Registration!</h4>

        <form  asp-action="register" asp-controller="Users" method="post">
            <br><span></span>
            <label asp-for="fname"></label>
            <br>
            <input  class =inputbox type="text" asp-for="fname">
            <br>
            <span asp-validation-for="fname"></span>
            <br>
            <label asp-for="lname"></label>
            <br>
            <input  class =inputbox type="text" asp-for="lname">
            <br>
            <span asp-validation-for="lname"></span>
            <br>
            <label asp-for="email" ></label>
            <br>
            <input  class =inputbox type="text" asp-for="email">
            <br>
            <span asp-validation-for="email"></span>
            <br>
            <label asp-for="password" ></label><br>
            <input  class =inputbox type="password" asp-for="password">
            <br>
            <span asp-validation-for="password"></span>
            <br>
            <label asp-for="confirm_password"></label><br>
            <input  class =inputbox type="password" asp-for="confirm_password">
            <br>
            <br>
            <input type="submit" value="Register"><br>
            </form>
            <img height="" width="" border="0" alt=""src ="~/images/1.jpeg"></img>
        </div>
        <partial name="loginUser"/>
        
        <img height="" width="" border="0" alt="" src="https://www.xyz.com/images/123.jpg"></img>
